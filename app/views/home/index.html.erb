<%= render :partial => 'orien'%>
<% if is_logged_in? %>
  <!-- START BODY -->
  <div id="body">
    <div id="home_content_area">
      <!-- THIS VERSION OF THE HOMEPAGE APPEARS WHEN THE USER _IS_ LOGGED IN -->
      <!-- We need to use a table so we can use the joint cells to control heights when tabs are changed -->
      <table cellpadding="0" cellspacing="0" border="0" id="top_panel">
        <tr>
          <td id="dashboard" valign="top" style='background-image:url(../images/image/home_banner_bg3.jpg)'>
            <!-- Dashboard -->
            <%= render :partial => 'dashboard' %>
            <!-- End Dashboard -->
          </td>
          <td id="spacer">&nbsp;</td>
          <td id="community" valign="top">
            <!-- Community -->
            <%= render :partial => 'community' %>
            <!-- End Community -->
          </td>
        </tr>
      </table>
      <!-- Start Browsing Panel -->
      <%= render :partial => 'panel' %>
      <!-- End Browsing Panel -->
    </div>
    <div style="clear:both;"></div>
  </div>
  <!-- END BODY -->
<% else %>
  <div id="body">
    <div id="home_content_area">
      <!-- THIS VERSION OF THE HOMEPAGE APPEARS WHEN THE USER IS not LOGGED IN -->
      <%= render :partial => 'started' %>
      <!-- End started -->
      <!-- Welcome Panel -->
      <%= render :partial => 'panel'%>
      <!-- Start Browsing Panel -->
    </div>
    <div style="clear:both;"></div>
  </div>
<% end %>
